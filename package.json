{
  "name": "app-connections-rds",
  "version": "1.0.0",
  "description": "Gerenciador de Conex√µes AWS via SSM",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.app.app",
    "productName": "app-connections-rds",
    "directories": {
      "output": "dist"
    },
    "files": [
      "index.js",
      "renderer/**/*",
      "utils/**/*"
    ],
    "extraResources": [
      {
        "from": "connections.json",
        "to": "connections.json"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "deleteAppDataOnUninstall": true
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
